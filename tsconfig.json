{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@screens/*": ["./src/screens/*"],
      "@redux/*": ["./src/redux/*"],
      "@components/*": ["./src/components/*"],
      "@theme/*": ["./src/theme/*"],
      "@utils/*": ["./src/utils/*"],
      "@UI/*": ["./src/UI/*"],
      "@customTypes/*": ["./src/types/*"]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/types/**/*.d.ts",
    "babel.config.js"
  ]
}
